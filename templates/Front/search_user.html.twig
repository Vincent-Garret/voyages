{% extends "base.html.twig" %}

{% block body %}

    <form action="{{ path('search_user') }}" method="GET">
        <label>
            <input class="searchBar" type="text" name="search" placeholder="RECHERCHER">
        </label>
        <button class="btnSearch" type="submit">
            <i class="fa fa-search"></i>
        </button>
    </form>

    {% for user in users %}

    <ul>
        <li>{{ user.userName }}</li>
    </ul>
        
    {% endfor %}

{% endblock %}